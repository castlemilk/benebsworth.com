  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy.prod": "gatsby clean && gatsby build && s3-deploy './public/**' --cwd './public/' --profile ben --region ap-southeast-2 --bucket benebsworth.com --deleteRemoved --gzip && s3-deploy './static/**' --cwd './static/' --region ap-southeast-2 --bucket benebsworth.com --gzip && npm run clear",
    "deploy.next": "gatsby clean && gatsby build && s3-deploy './public/**' --cwd './public/' --profile ben --region ap-southeast-2 --bucket next.benebsworth.com --deleteRemoved --gzip && s3-deploy './static/**' --cwd './static/' --region ap-southeast-2 --bucket next.benebsworth.com --gzip",
    "clear": "aws --profile ben cloudfront create-invalidation --distribution-id E30L2WGIX5OPQG --paths '/*'"
  }
